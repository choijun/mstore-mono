{
  "name": "exmgr",
  "version": "1.0.0",
  "description": "Monolithic application sample using Spring Boot",
  "author": "huynguyen8505@gmail.com",
  "license": "MIT",
  "repository": "https://github.com/huytrongnguyen/mstore-mono",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --port 3000 --proxy-config ./client/src/ng-ts/environments/proxy.conf.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-function-parameter-decorators",
      "transform-object-rest-spread",
      ["root-import", { "rootPathSuffix": "src" }]
    ],
    "ignore": "node_modules"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "@angular/animations": "6.1.3",
    "@angular/common": "6.1.3",
    "@angular/compiler": "6.1.3",
    "@angular/core": "6.1.3",
    "@angular/forms": "6.1.3",
    "@angular/http": "6.1.3",
    "@angular/platform-browser": "6.1.3",
    "@angular/platform-browser-dynamic": "6.1.3",
    "@angular/router": "6.1.3",
    "core-js": "2.5.7",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-function-parameter-decorators": "1.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-root-import": "6.1.0",
    "babel-preset-env": "1.7.0",
    "@angular-devkit/build-angular": "0.7.4",
    "@angular/cli": "6.1.4",
    "@angular/compiler-cli": "6.1.3",
    "@angular/language-service": "6.1.3",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.7.1",
    "babelify": "8.0.0",
    "bootstrap": "4.1.3",
    "browserify": "16.2.2",
    "codelyzer": "4.4.4",
    "express": "4.16.3",
    "font-awesome": "4.7.0",
    "gulp": "3.9.1",
    "gulp-sass": "4.0.1",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "3.0.1",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "jquery": "3.3.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.0",
    "popper.js": "1.14.4",
    "protractor": "5.4.0",
    "proxy-middleware": "0.15.0",
    "stringify": "5.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "vinyl-source-stream": "2.0.0",
    "yarn": "1.9.4"
  }
}
